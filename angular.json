{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ProyectoFinal-ISI-NicolasScandolo": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ProyectoFinal-ISI-NicolasScandolo",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "./node_modules/bootstrap/dist/css/bootstrap.min.css",
              "src/assets/bootstrap-3.3.7/bootstrap-3.3.7-dist/css/bootstrap.min.css",
              "src/assets/css/main.css",

              "src/assets/js/plugins/bootsnav_files/skins/color.css",

            "src/assets/js/plugins/bootsnav_files/css/animate.css",

             "src/assets/js/plugins/bootsnav_files/css/bootsnav.css",

             "src/assets/js/plugins/bootsnav_files/css/overwrite.css",

             "src/assets/js/plugins/owl-carousel/owl.carousel.css",

             "src/assets/js/plugins/owl-carousel/owl.theme.css",

             "src/assets/js/plugins/owl-carousel/owl.transitions.css",

             "src/assets/js/plugins/Magnific-Popup-master/Magnific-Popup-master/dist/magnific-popup.css"


            ],
            "scripts": [
              "./node_modules/jquery/dist/jquery.slim.min.js",
              "./node_modules/popper.js/dist/umd/popper.min.js",
              "./node_modules/bootstrap/dist/js/bootstrap.min.js",



               "https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js",

               "src/assets/bootstrap-3.3.7/bootstrap-3.3.7-dist/js/bootstrap.min.js",

               "src/assets/js/plugins/owl-carousel/owl.carousel.min.js",

               "src/assets/js/plugins/bootsnav_files/js/bootsnav.js",

               "src/assets/js/plugins/typed.js-master/typed.js-master/dist/typed.min.js",

               "https://maps.googleapis.com/maps/api/js",

               "src/assets/js/plugins/Magnific-Popup-master/Magnific-Popup-master/dist/jquery.magnific-popup.js",

               "src/assets/js/plugins/particles.js-master/particles.js-master/particles.min.js",

               "src/assets/js/particales-script.js",

               "src/assets/js/main.js"




            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ProyectoFinal-ISI-NicolasScandolo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ProyectoFinal-ISI-NicolasScandolo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ProyectoFinal-ISI-NicolasScandolo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ProyectoFinal-ISI-NicolasScandolo-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "ProyectoFinal-ISI-NicolasScandolo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ProyectoFinal-ISI-NicolasScandolo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ProyectoFinal-ISI-NicolasScandolo"
}
